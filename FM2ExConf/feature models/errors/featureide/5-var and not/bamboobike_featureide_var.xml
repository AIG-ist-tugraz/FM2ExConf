<featureModel>
	<properties>
		<graphics key="showhiddenfeatures" value="true"/>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="horizontal"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="1"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="Bamboo Bike">
			<graphics key="collapsed" value="false"/>
			<alt mandatory="true" name="Frame">
				<feature name="Female"/>
				<feature name="Male"/>
				<feature name="Step-through"/>
			</alt>
			<and mandatory="true" name="Brake">
				<feature mandatory="true" name="Back-pedal"/>
			</and>
			<feature name="Engine"/>
			<feature name="Drop Handlebar"/>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<var1>Engine</var1>
			</imp>
		</rule>
		<rule>
			<imp>
				<var1>Back-pedal</var1>
				<var>Engine</var>
			</imp>
		</rule>
	</constraints>
</featureModel>
